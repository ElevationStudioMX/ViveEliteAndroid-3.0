<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main_preview_image"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#000000"
    tools:context="com.example.desarrollo_elevation.viveelite.MainActivity_preview_image">


    <com.example.photogesturelibrary.PhotoView
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:id="@+id/idphotopreview"
        >

    </com.example.photogesturelibrary.PhotoView>

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbarpreviewimagen"

        android:title=""
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#80000000"
        android:minHeight="?attr/actionBarSize"
        android:subtitleTextColor="#fff"
        app:theme="@style/Theme.AppCompat"
        tools:ignore="UnusedAttribute"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"


         />









</AbsoluteLayout>
