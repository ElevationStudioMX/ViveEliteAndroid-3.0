<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main_webview_qr"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.desarrollo_elevation.viveelite.MainActivity_webviewQR">




    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbarwebviewQR"
        android:title="Promociones"
        android:background="@drawable/backgraund"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:theme="@style/Theme.AppCompat"
        android:minHeight="?attr/actionBarSize"
        tools:ignore="UnusedAttribute"
        />

    <WebView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/webviewqr"
       android:layout_below="@+id/toolbarwebviewQR"
        >


    </WebView>


</RelativeLayout>
